{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "skipLibCheck": true,
    "noImplicitAny": false,
    "paths": {
      "@/*": [
        "./"
      ],
      "app/*": [
        "./app/*"
      ]
    },
    "typeRoots": [
      "./node_modules/@types",
      "./"
    ]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.js",
    "**/*.jsx",
    "minimatch.d.ts"
  ]
}
